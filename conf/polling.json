  {
    "Name": "LANポート送受信",
    "Type": "snmp",
    "Mode": "ps",
    "Params": "ifInOctets.$i,ifOutOctets.$i",
    "Script": "Mbps=8*(ifInOctets_PS+ifOutOctets_PS)/1000000;Mbps < 1000",
    "Level": "low",
    "Descr": "LANポートの送受信の合計量をMbps単位で測定",
    "AutoMode": "index:ifIndex"
  },
  {
    "Name": "LANポート送受信(HC)",
