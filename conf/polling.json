    "Level": "low",
    "Descr": "CPU平均使用率"
  },
  {
    "Name": "ストレージ使用率",
    "Type": "snmp",
    "Mode": "get",
    "Params": "hrStorageSize.$i,hrStorageUsed.$i",
    "Script": "rate=hrStorageSize > 0 ? 100.0*(hrStorageUsed/hrStorageSize):0.0;rate > 90.0",
