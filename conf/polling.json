  {
    "Name": "LANポート送受信",
    "Type": "snmp",
    "Mode": "ps",
    "Params": "ifInOctets.$i,ifOutOctets.$i",
    "Script": "Mbps=8*(ifInOctets+ifOutOctets)/1000000;Mbps < 1000",
    "Level": "low",
    "AutoMode": "snmp:index",
    "Descr": "LANポートの送受信の合計量をMbps単位で測定"
  },
  {
    "Name": "LANポート受信",
    "Type": "snmp",
    "Mode": "ps",
    "Params": "ifInOctets.$i",
    "Script": "rxbytesps=ifInOctets;rxbytesps > 0",
    "Level": "low",
    "AutoMode": "snmp:index",
    "Descr": "LANポート受信データ量"
  },
  {
    "Name": "LANポート送信",
    "Type": "snmp",
    "Mode": "ps",
    "Params": "ifOutOctets.$i",
    "Script": "txbytesps=ifOutOctets;txbytesps > 0",
    "Level": "low",
    "AutoMode": "snmp:index",
    "Descr": "LANポート送信データ量"
  },
  {
    "Name": "CPUコア単位使用率",
    "Type": "snmp",
    "Mode": "get",
    "Params": "hrProcessorLoad.$i",
    "Script": "cpu=hrProcessorLoad; cpu < 90.0",
    "Level": "low",
    "AutoMode": "snmp:index",
    "Descr": "CPUコア単位使用率"
  },
  {
    "Name": "CPU平均使用率",
